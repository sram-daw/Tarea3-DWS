@model IEnumerable<RamiloAlonsoSaraTarea3.Models.Profesores>

@{
    ViewData["Title"] = "Profesores";
}

<h1>Profesores</h1>

<table class="table">
    <thead>
        <tr>
            <th>
               ID
            </th>
            <th>
               Nombre
            </th>
            <th>
               Edad
            </th>
            <th>
               Materia
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Edad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Materia)
            </td>
            <td>
                @Html.ActionLink("Detalles", "Detalles", new { id=item.Id})
            </td>
        </tr>
}
    </tbody>
</table>
